{
  "name": "react-gensvg",
  "version": "0.0.3",
  "description": "A tsx/jsx file generator for svg files",
  "author": "tizee <pobomp@gmail.com>(https://tizee.github.io)",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist",
    "cli"
  ],
  "homepage": "https://github.com/tizee/react-gensvg#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/tizee/react-gensvg"
  },
  "bugs": {
    "url": "https://github.com/tizee/react-gensvg"
  },
  "scripts": {
    "ts-check": "tsc --noEmit",
    "ts-check:watch": "tsc --noEmit --watch",
    "build": "npx tsc && npx rollup -c",
    "dev": "npx rollup -c -w",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm test"
    }
  },
  "devDependencies": {
    "@babel/cli": "^7.10.1",
    "@babel/core": "^7.10.2",
    "@babel/plugin-proposal-class-properties": "^7.10.1",
    "@babel/preset-env": "^7.10.2",
    "@babel/preset-typescript": "^7.10.1",
    "@rollup/plugin-babel": "^5.0.3",
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-node-resolve": "^8.0.1",
    "@rollup/plugin-typescript": "^5.0.2",
    "@types/jest": "^26.0.4",
    "@types/prettier": "^2.0.2",
    "@types/svg-parser": "^2.0.1",
    "@types/yargs": "^15.0.5",
    "@typescript-eslint/eslint-plugin": "^3.2.0",
    "@typescript-eslint/parser": "^3.2.0",
    "del": "^5.1.0",
    "eslint": "^7.2.0",
    "eslint-config-prettier": "^8.0.0",
    "eslint-plugin-prettier": "^3.1.3",
    "husky": "^4.2.5",
    "jest": "^26.1.0",
    "rollup": "^2.16.1",
    "rollup-plugin-copy": "^3.3.0",
    "rollup-plugin-uglify": "^6.0.4",
    "typescript": "^3.9.5"
  },
  "dependencies": {
    "prettier": "^2.0.5",
    "react-domproperty": "^1.0.4",
    "svg-parser": "^2.0.4",
    "yargs": "^15.4.0"
  },
  "keywords": [
    "cli",
    "generator",
    "code-generator",
    "svg",
    "react",
    "typescript"
  ],
  "bin": {
    "svgcli": "cli/index.js"
  },
  "engines": {
    "node": ">=12",
    "npm": ">=6"
  }
}
